{
    "name": "danyelkeddah/chuck-norris-jokes",
    "description": "Create random Chuck Norris Jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danyel Alkeddah"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "DanyelKeddah\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanyelKeddah\\ChuckNorrisJokes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanyelKeddah\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "DanyelKeddah\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}